{ "manifest_version": 2,
    "name": "Image Finder",
    "description": "Find all images links in a page!",
    "icons": { "16": "images/image-16.png",
         "32": "images/image-32.png",
          "128": "images/image-128.png" },
    "version": "1.0.0",
        "page_action": {
            "default_popup": "popup.html"
        },

        "permissions": ["activeTab", "declarativeContent", "storage"]
    }
